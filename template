<script>

var showMenu = function() {
var stuff = $(".logoHolder, .department, .menu, #postSorting, .socialHolder");
 $(".hamburger").on("click" , function(){
    $(".hamburger").css("display" , "none");
    stuff.css("display" , "block");
    $(".container").removeClass("containerMove");
     $(".blackBar").removeClass("menuHide");
     $(".closer").css("display" , "block");
 });
 
 $(".closer").on("click" , function(){
     $(".blackBar").addClass("menuHide");
     $(".closer").css("display" , "none");
     $(".hamburger").css("display" , "block");
     stuff.css("display" , "none");
     $(".container").addClass("containerMove");
 });
}

  $(".department, .otherTumblrs").on("click", function(){
     $(".otherTumblrs").toggleClass("showOther")
  });

showMenu();

// Look at each video
// Find the height
// Find the width
// If the Height is greater than the width change css "padding-bottom"
var videoSizer = function(){
    var videoLength = $(".videoWrapper iframe").length;
    var videos = $(".videoWrapper iframe");
    var videoHolder = $(".videoWrapper");
    for(i=0; i <= videoLength; i++){
        var videoHeight = $(videos[i]).attr("height");
        var videoWidth = $(videos[i]).attr("width");
        if(videoHeight >= videoWidth){
            $(videoHolder[i]).css("padding-bottom", "120%");
        }
    }
}
videoSizer();
    


  
var lightBox = function() {
      $(".lightBoxBack").on("click", function(){
          $(".lightBoxBack").removeClass("lightBoxShow");
          $(".lightBoxBack img").remove();
      })
      var lightBoxable = $(".lightBoxable");
      var lightCount = $(".lightBoxable").length;
      for(i=0; i <= lightCount; i++){
	    $(lightBoxable[i]).on("click", function(){
	    $(".lightBoxBack").addClass("lightBoxShow");
		var lightIndex = lightBoxable.index(this);
		$(lightBoxable[lightIndex]).clone().appendTo(".lightBoxBack");
	})
    }
  };
lightBox();
 
 
 
var selector = function(){
    var pathArray = window.location;
    var options = $('option');
    options.each(function(){
        var value = $(this).attr("value");
        if(value == pathArray) {
            $(this).prop("selected", "selected");
        }
    })   
    
}
selector();  
 
 
 
  
  
function resizePhotosets(){
    var photosetCheck = $(".html_photoset").length;
    if(photosetCheck != 0){
    var parentWidth = document.body.querySelector('.html_photoset').offsetWidth; 
    var photosets = document.body.querySelectorAll('iframe.photoset');
    for(var i = 0; i < photosets.length; ++i){
        var photoset = photosets[i];
        photoset.width = parentWidth;
    }
    }
}
window.onload = resizePhotosets;
window.onresize = resizePhotosets;


$(function(){
    // bind change event to select
$('#category').bind('change', function () {
    var url = $(this).val(); // get selected value
    if (url) { // require a URL
        window.location = url; // redirect
    }
    return false;
      });
});



setTimeout(function(){
    var heightLeft = $(".size-1of2:first-child").css("height");
    var heightRight = $(".size-1of2:last-child").css("height");
    if(heightLeft >= heightRight){
    $(".size-1of2:first-child").css("border-right", "1px solid {color:Menu Bar}");
}else{
    $(".size-1of2:last-child").css("border-left", "1px solid {color:Menu Bar}");
}}, 1200);
    
</script>


